{   
    "title": "gitbook",
    "description": "Ralph's gitbook",
    "language": "en",
    "pluginsConfig": {
        "fontSettings": {
          "theme": "white",
          "family": "msyh",
          "size": 2
        } 
    },
    "plugins": [
      "multipart",
      "yahei",
      "katex",
      "search",
      "splitter",
      "collapsible-chapters"
   ],
  "pdf": {
    "pageNumbers": true, 
    "fontFamily": "Arial",
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 30,
      "left": 30,
      "top": 30,
      "bottom": 50
    }
  }
}
